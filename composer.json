{
    "name": "docler-labs/api-client-exception",
    "version": "2.0.0",
    "description": "SDK Generator - API Client Exception",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "rest",
        "api",
        "api-client",
        "http",
        "exception"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DoclerLabs\\ApiClientException\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoclerLabs\\ApiClientException\\Test\\Unit\\": "test/suite/unit/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
